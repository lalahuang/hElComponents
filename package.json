{
  "name": "hcomponents",
  "private": false,
  "version": "0.0.0",
  "keywords": [
    "vue3",
    "element-plus",
    "hcomponents",
    "table",
    "tag",
    "queryWapper"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "unpkg": "dist/index.umd.js",
  "style": "dist/index.css",
  "sideEffects": [
    "dist/index.css",
    "theme/*",
    "*.css"
  ],
  "scripts": {
    "comp:build": "pnpm -C packages/components build",
    "clean": "rimraf dist",
    "move": "node script/move.js",
    "build": "pnpm clean && pnpm build:css && pnpm build:theme && pnpm vite build   && pnpm publish:comp",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:css": "vite build --config vite.config.css.ts",
    "publish:comp": "gulp -f script/publish/gulpfile.js",
    "publish:utils": "gulp -f script/publishUtil/gulpfile.js",
    "build:theme": "vite build --config vite.config.theme.ts",
    "build:utils": "pnpm clean:utils && vite build --config vite.config.utils.ts && pnpm publish:utils",
    "clean:utils": "rimraf distutils",
    "move:utils": "node script/moveutils.js",
    "docs:dev": "pnpm run -C docs docs:dev"
  },
  "peerDependencies": {
    "@vueuse/core": "^9",
    "element-plus": "^2",
    "vue": "^3.2"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.1.0",
    "element-plus": "^2.2.13",
    "h-components-next": "workspace:^1.0.43",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "vue": "^3.2.41",
    "vue-clipboard3": "^2.0.0",
    "zhyswan-vuedraggable": "^4.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.4",
    "unplugin-vue-setup-extend-plus": "^0.3.2",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.4.1",
    "vue-tsc": "^0.39.5"
  }
}