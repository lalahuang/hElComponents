{
  "name": "hcomponents",
  "private": false,
  "version": "0.0.0",
  "keywords": [
    "vue3",
    "element-plus",
    "hcomponents",
    "table",
    "tag",
    "queryWapper"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/h-components.umd.js",
  "module": "lib/h-components.mjs",
  "exports": {
    ".": {
      "import": "./lib/h-components.mjs",
      "require": "./lib/h-components.umd.js"
    }
  },
  "scripts": {
    "comp:build": "pnpm -C packages/components build",
    "clean": "rimraf dist",
    "move": "node script/move.js",
    "build": "pnpm clean && pnpm build:css && vite build && pnpm build:lib   && pnpm move",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:css": "vite build --config vite.config.css.ts",
    "build:theme": "vite build --config vite.config.theme.ts"
  },
  "dependencies": {
    "@h-components/utils": "workspace:*",
    "@vueuse/core": "^9.1.0",
    "element-plus": "^2.2.13",
    "h-components-next": "workspace:^1.0.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "vue": "^3.2.37",
    "zhyswan-vuedraggable": "^4.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.4",
    "unplugin-vue-setup-extend-plus": "^0.3.2",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vue-tsc": "^0.39.5"
  }
}